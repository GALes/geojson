{
    "name": "geojson/geojson",
    "description": "GeoJSON implementation for PHP",
    "keywords": [
        "geojson",
        "geo"
    ],
    "homepage": "http://github.com/lorenzomar/geojson",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lorenzo Marzullo",
            "email": "marzullo.lorenzo@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "marc-mabe/php-enum": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
